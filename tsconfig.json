{
  "extends": "@tsconfig/react-native/tsconfig.json",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "*": [
        "src/*"
      ],
      "@components": [
        "src/components"
      ],
      "@routes": [
        "src/routes"
      ],
      "@screens": [
        "src/screens"
      ],
      "@store": [
        "src/store"
      ],
      "@interfaces": [
        "src/interfaces"
      ],
      "@utils": [
        "src/utils"
      ],
      "@translate/*": [
        "src/translate/*"
      ],
      "@theme": [
        "src/theme"
      ],
      "@providers": [
        "src/providers"
      ],
      "@assets/*": [
        "assets/*"
      ]
    },
    "allowSyntheticDefaultImports": true, /* Allow default imports from modules with no default export. This does not affect code emit, just typechecking. */
    "resolveJsonModule": true,
    "esModuleInterop": true, 
    "skipLibCheck": false 
  }
}